{
	"name": "$t:calendar-time",
	"version": "1.0.0",
	"types": ["date", "datetime", "datetime_updated", "datetime_created"],
  "icon": "event",
	"options": {
    "min": {
      "name": "$t:min",
      "comment": "$t:min_comment",
      "interface": "datetime"
    },
    "max": {
      "name": "$t:max",
      "comment": "$t:max_comment",
      "interface": "datetime"
    },
    "formatting": {
      "name": "$t:formatting",
      "comment": "$t:formatting_comment",
